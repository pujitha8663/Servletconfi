<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletConfigy</servlet-name>
  	<display-name>ServletConfigy</display-name>
  	<description></description>
  	<servlet-class>com.config.ServletConfigy</servlet-class>
  	 <init-param>
         <param-name>user</param-name>
         <param-value>puja</param-value>
     </init-param>
  	 <init-param>
         <param-name>password</param-name>
         <param-value>pujitha</param-value>
     </init-param>
  	
  	
  </servlet>
  <servlet>
  	<servlet-name>ContextEX</servlet-name>
  	<display-name>ContextEX</display-name>
  	<description></description>
  	<servlet-class>com.context.ContextEX</servlet-class>
  	
  		<context-param>
        <param-name>user</param-name>
        <param-value>puja</param-value>
    </context-param>
    <context-param>
        <param-name>password</param-name>
        <param-value>pujitha</param-value>
        
        </context-param>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletConfigy</servlet-name>
  	<url-pattern>/conf</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ContextEX</servlet-name>
  	<url-pattern>/cont</url-pattern>
  </servlet-mapping>
  <context-param>
        <param-name>Name</param-name>
        <param-value>puja</param-value>
    </context-param>
    <context-param>
        <param-name>City</param-name>
        <param-value>Huzurabad</param-value>
        
        </context-param>
  
  
</web-app>
